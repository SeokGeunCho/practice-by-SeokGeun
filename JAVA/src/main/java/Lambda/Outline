ì¸í„°í˜ì´ìŠ¤	                í˜•íƒœ	             ì„¤ëª…                   ë©”ì„œë“œ ì´ë¦„
Runnable	            () â†’ void	     ë§¤ê°œë³€ìˆ˜ X, ë°˜í™˜ X	        run()
Supplier<T>	            () â†’ T	         ë§¤ê°œë³€ìˆ˜ X, ë°˜í™˜ O	        get()
Consumer<T>	            (T) â†’ void	     ë§¤ê°œë³€ìˆ˜ O, ë°˜í™˜ X	        accept()
Function<T,R>          	(T) â†’ R	         ì…ë ¥ T â†’ ì¶œë ¥ R        	    apply()
Predicate<T>	        (T) â†’ boolean	 ì¡°ê±´ ê²€ì‚¬ (true/false)	    test()
BiFunction<T, U, R>	    (T, U) â†’ R	     ë‘ ì…ë ¥ â†’ ì¶œë ¥	            apply()


import java.util.function.*;

public class LambdaSummary {
    public static void main(String[] args) {

        // 1. Runnable - ë§¤ê°œë³€ìˆ˜ ì—†ìŒ, ë°˜í™˜ ì—†ìŒ
        Runnable run = () -> System.out.println("Runnable ì‹¤í–‰ ì¤‘ì…ë‹ˆë‹¤.");
        run.run(); // ğŸ‘‰ run() ë©”ì„œë“œ ì‹¤í–‰

        // 2. Supplier - ë§¤ê°œë³€ìˆ˜ ì—†ìŒ, ê°’ì„ ë°˜í™˜í•¨
        Supplier<String> sup = () -> "ê³µê¸‰ëœ ë¬¸ìì—´";
        System.out.println(sup.get()); // ğŸ‘‰ "ê³µê¸‰ëœ ë¬¸ìì—´"

        // 3. Consumer - ê°’ì„ ë°›ì•„ì„œ ì¶œë ¥ë§Œ (ë¦¬í„´ ì—†ìŒ)
        Consumer<String> con = name -> System.out.println("ì´ë¦„: " + name);
        con.accept("í™ê¸¸ë™"); // ğŸ‘‰ "ì´ë¦„: í™ê¸¸ë™"

        // 4. Function - ì…ë ¥ê°’ì„ ë°›ì•„ ë³€í™˜ í›„ ë°˜í™˜
        Function<Integer, String> func = i -> "ìˆ«ì: " + i;
        System.out.println(func.apply(100)); // ğŸ‘‰ "ìˆ«ì: 100"

        // 5. Predicate - ì¡°ê±´ ê²€ì‚¬ (true/false ë°˜í™˜)
        Predicate<String> isEmpty = str -> str.isEmpty();
        System.out.println(isEmpty.test(""));       // ğŸ‘‰ true
        System.out.println(isEmpty.test("hello"));  // ğŸ‘‰ false

        // 6. BiFunction - ë‘ ê°œì˜ ì…ë ¥ê°’ì„ ë°›ì•„ ê²°ê³¼ ë°˜í™˜
        BiFunction<String, String, Boolean> bf = (a, b) -> a.equals(b);
        System.out.println(bf.apply("java", "java"));   // ğŸ‘‰ true
        System.out.println(bf.apply("java", "hello"));  // ğŸ‘‰ false
    }
}
